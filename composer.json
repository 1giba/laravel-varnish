{
    "name": "spatie/laravel-varnish",
    "description": "Integrate Varnish in a Laravel application",
    "keywords": [
        "spatie",
        "laravel-varnish"
    ],
    "homepage": "https://github.com/spatie/laravel-varnish",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/process": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Varnish\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Varnish\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
